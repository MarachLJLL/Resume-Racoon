{
    "name": "Resume Racoon",
    "short_name": "Resume Racoon",
    "version": "1.0.0.0",
    "description": "Resume Racoon",
    "manifest_version": 3,
    "permissions": ["storage", "scripting", "activeTab","background"],
    "host_permissions": ["<all_urls>"],
    "action": {
        "default_popup": "index.html",
        "default_title": "Resume Racoon"
    },
    "web_accessible_resources": [
        {
            "resources": ["background.jpg", "images/background.jpg", "api.txt"],
            "matches": ["<all_urls>"]
        }
    ],
    "content_scripts": [
        {
            "matches": ["*://*.co/*"],
            "js": ["Jason/contentScript.js"]
        }
    ]
  }